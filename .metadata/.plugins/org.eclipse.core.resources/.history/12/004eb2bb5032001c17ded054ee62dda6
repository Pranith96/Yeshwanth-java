
public class StringExamples {

	public static void main(String[] args) {

		String s = "Hi";
		String s1 = "Hi";
		String s2 = s.concat("Hello");
		System.out.println(s2);
		s.toLowerCase();
		s.toUpperCase();
		if (s == s1) {
			System.out.println("123456");
		} else {
			System.out.println("1234");

		}
	}

}
